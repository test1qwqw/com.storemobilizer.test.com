<?xml version='1.0' encoding='utf-8'?>
<widget
  xmlns       = "http://www.w3.org/ns/widgets"
  xmlns:gap   = "http://phonegap.com/ns/1.0"
  id          = "com.storemobilizer.test.com"
  version     = "15.11.16"
  versionCode = "151116"
>
  <name>test</name>
  <author
    href="http://storemobilizer.com"
    email="info@storemobilizer.com">StoreMobilizer
  </author>
  <description>test.com for android</description>
  <preference name="phonegap-version" value="3.6.3"/>
  <preference name="fullscreen" value="true"/>
  <preference name="permissions" value="none"/>
  <feature    name="http://api.phonegap.com/1.0/file"/>

  <icon         src="icon.png" gap:platform="android" gap:qualifier="hdpi"/>
  <gap:splash   src="logo.png" gap:platform="android"/>
  <gap:platform name="android"/>
  <gap:plugin   name="org.apache.cordova.file"          source="pgb" version="1.3.1"/>
  <gap:plugin   name="org.apache.cordova.globalization" source="pgb" version="0.3.1"/>
  <gap:plugin   name="org.apache.cordova.inappbrowser"  source="pgb" version="0.5.2"/>
  <gap:plugin   name="phonegap-plugin-push"             source="npm" version="1.2.3"/>
  <gap:plugin   name="cordova-plugin-x-socialsharing"   source="npm" version="5.0.6"/>
  <access       origin="*"/>
</widget>